{
    // This launch configuration uses the Native Debug extension
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C: Remote Attach",
            "type": "gdb",
            "request": "attach",
            "executable": "${workspaceFolder}/blink_led.elf",
            "cwd": "${workspaceFolder}",
            "target": "localhost:1234",
            "remote": true,
            "gdbpath": "/usr/local/bin/gdb"
        }
    ]
}
